{
    "name": "shadiakiki1986/flysystem-gitrestapi",
    "description": "Flysystem adapter for git-rest-ap",
    "type": "library",
    "authors": [
        {
            "name": "Shadi Akiki",
            "email": "shadiakiki1986@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.5",
        "league/flysystem": "^1.0",
        "shadiakiki1986/git-rest-api-client": "^0.0.3"
    },
    "autoload": {
